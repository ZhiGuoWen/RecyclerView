<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wenzhiguo.recyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SeparateActivity" />
        <activity android:name=".WaterFallActivity" />
        <activity android:name=".BullActivity" />
        <activity android:name=".AnimationDeleteActivity" />
        <activity android:name=".ListViewActivity"/>
        <activity android:name=".StickyHeaderActivity"/>
    </application>

</manifest>